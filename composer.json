{
  "name": "restruct/silverstripe-admintweaks",
  "description": "Various admin tweaks & enhancements",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Restruct",
      "email": "dev@restruct.nl",
      "homepage": "https://restruct.nl"
    }
  ],
  "require": {
    "silverstripe/framework": "^4 | ^5",
    "guzzlehttp/guzzle": "^7.0"
  },
  "replace": {
    "micschk/silverstripe-cmstweaks": "1.*",
    "restruct/silverstripe-cmstweaks": "*"
  },
  "suggest": {
    "restruct/silverstripe-login-branding": "Reduce Silverstripe branding of the admin login and make it easily configurable",
    "symbiote/silverstripe-grouped-cms-menu": "Group CMS menu items into nested lists",
    "wedevelopnl/silverstripe-webp-images": "WEBP image support (output resized jpeg/png images as webp, GD with WEBP required)",
    "axllent/silverstripe-cms-tweaks": "Some complemental tweaks by Axllent"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\Silverstripe\\AdminTweaks\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist",
      "client/imgs"
    ]
  }
}
