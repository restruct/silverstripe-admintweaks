{
  "name": "restruct/silverstripe-admintweaks",
  "description": "Various admin tweaks & enhancements",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Restruct",
      "email": "dev@restruct.nl",
      "homepage": "https://restruct.nl"
    }
  ],
  "require": {
    "silverstripe/framework": "^5",
    "guzzlehttp/guzzle": "^7.5",
    "symbiote/silverstripe-gridfieldextensions": "4.1.5"
  },
  "replace": {
    "micschk/silverstripe-cmstweaks": "1.*",
    "restruct/silverstripe-cmstweaks": "*"
  },
  "suggest": {
    "axllent/silverstripe-cms-tweaks": "Some complemental tweaks by Axllent"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\Silverstripe\\AdminTweaks\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist",
      "client/imgs"
    ]
  }
}
